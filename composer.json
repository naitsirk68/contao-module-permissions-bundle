{
    "name": "naitsirk68/contao-module-permissions-bundle",
    "description": "module permissions extension for Contao Open Source CMS",
    "keywords": ["contao", "module", "permissions"],
    "type": "contao-module",
    "license": "LGPL-3.0+",
    "version": "2.0.0",
    "authors": [
        {
            "name": "Codefog",
            "homepage": "http://codefog.pl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "contao/core-bundle": ">=4.4",
        "symfony/framework-bundle": ">=3.3"
    },
    "require-dev": {
      "contao/manager-plugin": "^>=2.0",
      "doctrine/doctrine-cache-bundle": ">=1.3",
      "phpunit/phpunit": ">=5.7.26",
      "symfony/phpunit-bridge": ">=3.2"
    },
    "extra": {
      "contao-manager-plugin": "naitsirk68\\ContaoModulePermissionsBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
      "psr-4": {
        "naitsirk68\\ContaoModulePermissionsBundle\\": "src/"
      }
    }
}
